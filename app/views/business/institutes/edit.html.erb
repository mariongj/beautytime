 <div>
  <%= simple_form_for [:business, @institute], html: { multipart: true } do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :zipcode %>
    <%= f.input :picture1 %>
    <%= f.input :picture2 %>
    <%= f.input :picture3 %>
    <%= f.submit "Modifier votre institut", class: "btn btn-primary" %>
  <% end %>
</div>
